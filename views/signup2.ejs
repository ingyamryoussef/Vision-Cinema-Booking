            <form action="/database1" method="post" class="signupform">
                <br><br>
                <h3><b><u>Please fill the form to sign up</u></b></h3>
                <Label>Username:</Label>
                <input type="text" id='username' name='username' style="width: 20%;" size="30px">
                <br/><br/>
                <Label>E-mail:</Label>
                <input type="text" id='email' name='email'  style="width: 20%;" size="30px" multiple>
                <br/><br/>
                <Label>Password:</Label>
                <input type="password" id='password' name='password'  style="width: 20%;" size="30px">
                <br/><br/>
                <label>Date of birth:</label>
                <input type="date" id='dob' name='dob'  style="width: 20%;" size="30px">
                <br/><br/>
                <Label>Mobile number:</Label>
                <input type="text" id='mobnumber' name='mobnumber'  style="width: 20%;" size="30px">
                 <br/><br/>
                <label>Gender:</label>
                <input type="radio" id='male' name='gender' value="Male">Male
                <input type="radio" id='female' name='gender' value="Female">Female
                <br/><br/>
                <label>Way of paying:</label>
                <select name='wayofpaying' value="Credit">Credit Card>
                <option value='credit'>Credit Card</option>
                <option value='debit'>Debit Card</option>
                <option value='master'>MasterCard</option>
                </select>
                <br/><br/>
                <Label>Card details:</Label>
                
                 <br/><br/>
                 <button >Submit</button>
                 </form>
                 <br/><br/>
                <p style="margin-left: 20px;"><b>Already have an account?</b></p>
